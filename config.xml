<?xml version='1.0' encoding='utf-8'?>
<widget id="phuded.higherorlower" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Higher or Lower</name>
    <description>
        A card based drinking game.
    </description>
    <author email="phuded@gmail.com" href="https://www.linkedin.com/in/matthew-carter-05a54524/">
        Matthew Carter
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <screen src="res/screen/ios/Default@2x~universal~anyany.png" />
    <icon src="res/icon/icon.png" />
    <engine name="ios" spec="^4.5.5" />
</widget>
